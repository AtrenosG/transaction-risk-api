# Supabase Configuration
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_KEY=your-supabase-anon-key

# Server Configuration
HOST=0.0.0.0
PORT=8000
DEBUG=false

# Frontend Configuration
FRONTEND_URL=https://your-frontend-domain.vercel.app

# Webhook Configuration (Optional)
WEBHOOK_URL=https://your-agentic-ai-backend.com/webhook
WEBHOOK_SECRET=your-webhook-secret-key

# Additional Webhook URLs (Optional - comma separated)
ADDITIONAL_WEBHOOK_URLS=https://service1.com/webhook,https://service2.com/webhook

# Database Configuration (if using direct PostgreSQL connection)
# DATABASE_URL=postgresql://user:password@localhost:5432/dbname

# Security Configuration (Optional)
# JWT_SECRET_KEY=your-jwt-secret-key
# JWT_ALGORITHM=HS256
# JWT_EXPIRE_MINUTES=30

# Logging Configuration
LOG_LEVEL=info

# ML Model Configuration (Optional)
ML_MODEL_VERSION=1.0
RISK_THRESHOLD_LOW=40
RISK_THRESHOLD_HIGH=70

# Cache Configuration (Optional)
# REDIS_URL=redis://localhost:6379/0
# CACHE_TTL=3600

# Rate Limiting (Optional)
# RATE_LIMIT_REQUESTS=100
# RATE_LIMIT_WINDOW=60

# Monitoring (Optional)
# SENTRY_DSN=https://your-sentry-dsn
# ENABLE_METRICS=true
